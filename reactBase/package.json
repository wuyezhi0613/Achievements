{
  "name": "summit-web",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "live": "live-server ./build",
    "lint": "tslint ./src/**/*.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tslint ./src/**/*.tsx",
      "pre-push": "tslint ./src/**/*.tsx"
    }
  },
  "standard": {
    "globals": [
      "console",
      "Error",
      "config",
      "$",
      "antd",
      "woff"
    ],
    "properties": false,
    "parser": "babel-eslint",
    "ignore": [
      "./public/dist/",
      "./build/"
    ]
  },
  "browsers": [
    ">5%",
    "last 4 versions",
    "Firefox ESR",
    "Edge",
    "ie >= 9"
  ],
  "dependencies": {
    "@material-ui/core": "^3.0.2",
