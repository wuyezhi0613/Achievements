{
  "presets": [
    "es2015",
    "react",
    "env"
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    },
    "targets": {
      "browsers": ["last 2 versions", "safari >= 7", "iOS >= 8"]
    }
  },
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
      }
    ]
  ]
}